{
  "name": "html-sass-boilerplate",
  "version": "1.0.0",
  "description": "[jQuery]()\r [jquery.fancybox]()\r [normalize]()\r [modality]()\r [jquery.inputmask]()\r [timer.jquery]()\r [aos]()\r [Owl]()",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "browser-sync": "^2.27.7",
    "editorconfig": "^0.15.3",
    "editorconfig-checker": "^4.0.2",
    "html-validate": "^5.5.0",
    "postcss-cli": "^9.1.0",
    "postcss-csso": "^6.0.0",
    "postcss-import": "^14.0.2"
  },
  "scripts": {
    "test": "editorconfig-checker --exclude '.git|node_modules|DS_Store'",
    "start": "browser-sync start 'src' --server --no-notify --no-ui --cwd 'src' --files 'index.html,css/**/*'",
    "syles": "postcss src/css/index.css --use postcss-import --use postcss-csso --output dist/css/index.css",
    "build": "npm run styles",
    "deploy": "cd dist && rsync --archive --compress --delete user@website.com:/var/www/website.com/html/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boffeer-tools/html-sass-template.git"
  },
  "keywords": [
    "html",
    "landing\u001b[D\u001b[D\u001b[D\u001b[D\u001b[D\u001b[D\u001b[Dscss",
    "landing"
  ],
